{
    "name": "Nextcloud News Reader",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "unlimitedStorage",
        "alarms"
    ],
    "action": {
        "default_icon": {
            "32": "src/assets/favicon.png"
        }
    },
    "options_page": "src/page/index.html",
    "background": {
        "service_worker": "src/background/index.js",
        "type": "module",
        "persistent": true
    },
    "icons": {
        "128": "src/assets/favicon.png"
    }
}